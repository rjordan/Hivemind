# Environment variables for GitHub OAuth
# Copy this file to .env and fill in your actual values

# GitHub OAuth App Configuration
# You can create a GitHub OAuth App at: https://github.com/settings/applications/new
# Homepage URL: http://localhost:5173 (or your frontend URL)
# Authorization callback URL: http://localhost:5173/auth/callback

GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

HIVEMIND_DB_HOST=localhost
HIVEMIND_DB_USER=dbuser
HIVEMIND_DB_PASSWORD=password
